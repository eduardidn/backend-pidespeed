{
  "name": "backend-pidespeed",
  "version": "1.0.0",
  "description": "pidespeed backend ",
  "main": "index.js",
  "scripts": {
    "start": "export PORT=8080 ENV=PRODUCTON && node build/main.js",
    "dev": "nodemon --watch \"src/\" --exec \"ts-node src/main\" -e ts",
    "build": "yarn build:clean && tsc",
    "build:clean": "rimraf build",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint && yarn build"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduardidn/backend-pidespeed.git"
  },
  "author": "eduardidn",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^5.3.0",
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "hash.js": "^1.1.7",
    "helmet": "^3.22.0",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongoose": "^4.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.10",
    "mongoose-lean-virtuals": "^0.6.2",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "node-forge": "^0.10.0",
    "nodemailer": "^6.3.0",
    "promise-mysql": "^4.0.4",
    "socket.io": "^2.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-fileupload": "^1.1.3",
    "@types/helmet": "^0.0.47",
    "@types/morgan": "^1.9.1",
    "@types/mysql": "^2.15.5",
    "@types/node": "^14.0.27",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^1.19.1",
    "prettier": "^2.0.5",
    "prettier-tslint": "^0.4.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0"
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run lint:fix"
    ]
  },
  "bugs": {
    "url": "https://github.com/eduardidn/backend-pidespeed/issues"
  },
  "homepage": "https://github.com/eduardidn/backend-pidespeed#readme"
}
